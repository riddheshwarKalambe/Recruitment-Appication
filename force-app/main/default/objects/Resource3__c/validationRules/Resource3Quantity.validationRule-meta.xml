<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Resource3Quantity</fullName>
    <active>true</active>
    <errorConditionFormula>OR( Quantity__c  &lt; 100, Quantity__c  &gt; 150)</errorConditionFormula>
    <errorDisplayField>Quantity__c</errorDisplayField>
    <errorMessage>Quantity Cannot Be Less Then 100 And Greater Then 150</errorMessage>
</ValidationRule>
