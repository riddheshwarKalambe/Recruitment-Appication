<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Quantity_Validation</fullName>
    <active>true</active>
    <errorConditionFormula>Quantity__c &lt; 100</errorConditionFormula>
    <errorDisplayField>Quantity__c</errorDisplayField>
    <errorMessage>Quantity cannot be less then 100.</errorMessage>
</ValidationRule>
